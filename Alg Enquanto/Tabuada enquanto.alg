algoritmo "tabuada"
// Função :Função de tabuada
// Autor : Chrystian Moté Barbosa
// Data : 5/10/2010
// Seção de Declarações
var
num: inteiro
resul,cont: inteiro
inicio
cont <- 0
escreva("Informe um Número que queira saber sua Tabuada: ")
leia(num)
enquanto cont<10 faca
  cont <- cont + 1
  resul <- num*cont
    escreva(num, "X" ,cont, "=" ,resul)
    fimenquanto
    fimalgoritmo
