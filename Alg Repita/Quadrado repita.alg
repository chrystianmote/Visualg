algoritmo "quadrado"
// Função :Quadrado de 15 a 200
// Autor : Chrystian Moté Barbosa
// Data : 25/10/2010
// Seção de Declarações
var
 num: inteiro
 qua: real
 inicio

  num <- 15
      repita
            qua <- num ^ 2
            num <-  num + 1
            escreval(qua)
      ate num > 200


 fimalgoritmo
